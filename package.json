  "scripts": {
    "dev": "NODE_ENV=test yarn clean &&NODE_DEV=local tsc-watch -p ./tsconfig.server.json --onSuccess 'node ./bin/server/index.js' --onFailure 'echo Beep! Compilation Failed' --compiler typescript/bin/tsc",
    "build": "yarn clean && next build && tsc --project tsconfig.server.json",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node ./bin/server/index.js",
    "build:client-analyze": "cross-env NODE_ENV=production ANALYZE=true next build",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "clean": "yarn clean:server && yarn clean:client",
    "clean:server": "rimraf bin",
    "clean:client": "rimraf .next",
    "precommit": "yarn clean && next build && tsc --project tsconfig.server.json"
